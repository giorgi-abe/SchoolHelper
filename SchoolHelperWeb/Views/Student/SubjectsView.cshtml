@model IEnumerable<SchoolHelperDomainModels.Abstraction.ISubject>
@{
    ViewData["Title"] = "SubjectsView";
    Layout = "~/Views/Shared/_logedInLayout.cshtml";
}
@section Styles {
    <link href="@Url.Content("~/css/studentview.css")" rel="stylesheet" type="text/css" />
}
<section class="addsubject-section" data-stellar-background-ratio="0.5">
    <div class="overlay"></div>
    <div class="container">
        <div class="addsubject-box">
            <a href="#" class="btn btn-primary">Go somewhere</a>
        </div>
    </div>
</section>
<section class="subject-list">
    <div class="container">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col">TeacherName</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <th scope="row">@item.Name</th>
                        <td>@item.TeacherName</td>
                        <td>
                            <a href="">Update</a>
                            <a href="@Url.Action("Grading","GradeController",item.Key)">Grades</a>
                            <a href="@Url.Action("Delete", "StudentController", item.Key)">
                                <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-trash" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm2.5 0a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm3 .5a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0V6z" />
                                    <path fill-rule="evenodd" d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1v1zM4.118 4L4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4H4.118zM2.5 3V2h11v1h-11z" />
                                </svg>
                            </a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>

    </div>
</section>

